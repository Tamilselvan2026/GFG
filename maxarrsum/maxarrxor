# print(maxx)
prefix=[0]*(n+1)
for i in range(n):
    prefix[i+1]=prefix[i]^arr[i]
for i in range(n-k+1):
    curr_xor=prefix[i+k]^prefix[i]
    maxx=max(maxx,curr_xor)
return maxx

